{
    "env": {
        "defaultIncludePaths": [
            "~/Programs/boost/boost_1_76_0",
            "${workspaceFolder}",
            "${workspaceFolder}/Aurora/include",
            "${workspaceFolder}/Aurora/include",
            "${workspaceFolder}/Aurora/include/../",
            "${workspaceFolder}/Aurora/lib/ctre/single-header",
            "${workspaceFolder}/Aurora/lib/embedded_template_library/etl/include",
            "${workspaceFolder}/Aurora/lib/littlefs/lfs",
            "${workspaceFolder}/Aurora/lib/type_safe/include",
            "${workspaceFolder}/Aurora/lib/nanoprintf",
            "${workspaceFolder}/lib",
            "${workspaceFolder}/lib/Chimera/../",
            "${workspaceFolder}/lib/Chimera/include",
            "${workspaceFolder}/lib/Chimera/source/**",
            "${workspaceFolder}/lib/ChimeraSim/include",
            "${workspaceFolder}/lib/CommonTools/cpputest/include",
            "${workspaceFolder}/lib/CommonTools/googletest",
            "${workspaceFolder}/lib/CommonTools/googletest/googletest/googlemock/include",
            "${workspaceFolder}/lib/CommonTools/googletest/googletest/googletest/include"
        ],
        "defaultDefines": [
            "CHIMERA_SIMULATOR",
            "CHIMERA_VIRTUAL",
            "DEBUG",
            "DISABLE_DEBUG_BREAK",
            "SIMULATOR",
            "UNICODE",
            "USING_NATIVE_THREADS",
            "_DEBUG",
            "_UNICODE",
            "__GNUC__"
        ],
        "defaultEmbeddedCStandard": "gnu17",
        "defaultEmbeddedCppStandard": "gnu++20"
    },
    "configurations": [
        {
            "name": "Simulator",
            "includePath": [
                "${defaultIncludePaths}"
            ],
            "defines": [
                "${defaultDefines}"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}