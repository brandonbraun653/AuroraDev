{
    "env": {
        "defaultIncludePaths":[
            "${workspaceFolder}",
            "${workspaceFolder}/Aurora/include",
            "${workspaceFolder}/Aurora/include",
            "${workspaceFolder}/Aurora/include/../",
            "${workspaceFolder}/Aurora/lib/ctre/single-header",
            "${workspaceFolder}/Aurora/lib/embedded_template_library/etl/include",
            "${workspaceFolder}/Aurora/lib/littlefs/lfs",
            "${workspaceFolder}/Aurora/lib/type_safe/include",
            "${workspaceFolder}/Aurora/lib/uLog",
            "${workspaceFolder}/lib",
            "${workspaceFolder}/lib/Chimera/../",
            "${workspaceFolder}/lib/Chimera/include",
            "${workspaceFolder}/lib/CommonTools/cpputest/include",
            "${workspaceFolder}/lib/CommonTools/googletest",
            "${BOOST_ROOT}/boost_1_70_0"
        ],
        "defaultDefines":[
            "UNICODE",
            "USING_NATIVE_THREADS",
            "CHIMERA_SIMULATOR",
            "DEBUG",
            "_DEBUG",
            "_UNICODE",
            "__GNUC__"

        ],
        "defaultEmbeddedCStandard": "gnu17",
        "defaultEmbeddedCppStandard": "gnu++20"
    },

    "configurations": [
        {
            "name": "Simulator",
            "includePath": ["${defaultIncludePaths}"],
            "defines": [
                "${defaultDefines}"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++20"
        }
    ],
    "version": 4
}
